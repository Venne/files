{
	"name": "venne/files",
	"description": "Venne system administration",
	"keywords": ["nette", "venne", "system", "administration"],
	"homepage": "http://venne.cz",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Josef Kříž",
			"homepage": "http://josef-kriz.cz"
		}
	],
	"support": {
		"email": "info@venne.cz",
		"issues": "https://github.com/Venne/venne/issues"
	},
	"require": {
		"php": ">=5.4.0",
		"venne/venne": "~3.0.0"
	},
	"require-dev": {
		"venne/tester": "~1.1.0"
	},
	"autoload": {
		"psr-4": {
			"Venne\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"VenneTests\\": "tests/src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.0-dev"
		},
		"venne": {
			"configuration": {
				"extensions": {
					"files": "Venne\\Files\\DI\\FilesExtension"
				}
			}
		}
	}
}